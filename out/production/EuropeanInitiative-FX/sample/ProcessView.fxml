<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="236.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProcessViewController">
   <children>
      <Button fx:id="inputBtn" layoutX="30.0" layoutY="38.0" mnemonicParsing="false" onAction="#readInputFile" prefHeight="25.0" prefWidth="65.0" text="Input" />
      <Button fx:id="outputBtn" layoutX="30.0" layoutY="84.0" mnemonicParsing="false" onAction="#readOutputFile" prefHeight="25.0" prefWidth="65.0" text="Output" />
      <TextField fx:id="intputFileTxt" layoutX="109.0" layoutY="38.0" prefHeight="26.0" prefWidth="474.0" />
      <TextField fx:id="outputFiedlTxt" layoutX="109.0" layoutY="84.0" prefHeight="26.0" prefWidth="474.0" />
      <Button fx:id="processBtn" layoutX="220.0" layoutY="153.0" mnemonicParsing="false" onAction="#launchProcess" text="Process" />
      <Button fx:id="cancelBtn" layoutX="334.0" layoutY="153.0" mnemonicParsing="false" onAction="#cancelProcess" text="Cancel" />
      <Label fx:id="statusLabel" layoutX="172.0" layoutY="118.0" text="Select Input and output file then click on process" />
      <ProgressBar fx:id="progressIndicator" layoutX="20.0" layoutY="196.0" prefHeight="20.0" prefWidth="568.0" />
   </children>
</AnchorPane>
